{
    "manifest_version" : 2,
    "name": "Filter Extension",
    "version": "0.1",
    "background" : {
        "scripts": ["content.js"]
    },

    "content_scripts" : [
        {
            "matches" : [
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ]
}

